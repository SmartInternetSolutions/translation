{
    "name": "SmartInternetSolutions/translation",
    "description": "A Translation package for Laravel 4 with database, cache and multiple website support. This package is based on waavi/translation and has been extended by maksymcierzniak for multi-website support.",
    "license": "MIT",
    "authors": [
        {
            "name": "Waavi",
            "email": "info@waavi.com"
        },
        {
            "name": "maksymcierzniak"
        },
        {
            "name": "Smart Internet Solutions",
            "email": "opensource@smartinternetsolutions.net"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.x",
        "illuminate/translation": "4.x",
        "illuminate/database": "4.x",
        "waavi/model": "dev-master"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "SmartInternetSolutions\\Translation": "src/"
        }
    },
    "minimum-stability": "dev"
}
